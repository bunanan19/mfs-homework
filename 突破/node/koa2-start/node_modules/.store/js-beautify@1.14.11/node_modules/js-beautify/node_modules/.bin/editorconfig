#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/86153/Desktop/mfs-homework/突破/node/koa2-start/node_modules/.store/editorconfig@1.0.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/86153/Desktop/mfs-homework/突破/node/koa2-start/node_modules/.store/editorconfig@1.0.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../editorconfig@1.0.4/node_modules/editorconfig/bin/editorconfig" "$@"
else
  exec node  "$basedir/../../../../../editorconfig@1.0.4/node_modules/editorconfig/bin/editorconfig" "$@"
fi
